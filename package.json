{
  "name": "blog-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://Riddon.github.io/Blog_dev/",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix .",
